--
-- Table structure for table `Status`
--

DROP TABLE IF EXISTS `Status`;
CREATE TABLE `Status` (
  `Id` bigint(20) NOT NULL auto_increment,
  `Name` text,
  `Class` text,
  `Sequence` int(11) default NULL,
  `Prog_Enum` int(11) default NULL,
  PRIMARY KEY  (`Id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

